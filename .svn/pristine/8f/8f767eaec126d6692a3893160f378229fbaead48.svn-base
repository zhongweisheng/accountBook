body,button,dd,dt,dl,form,h1,h2,h3,h4,h5,h6,input,li,ol,p,select,table,td,textarea,ul{margin:0;padding:0;}
html{font-size:20px;-ms-touch-action:none;}
body,button,input,select,textarea{font-family:"苹方","圆体-简","方正兰亭细黑简体","微软雅黑",Verdana,Arial,Helvetica,sans-serif;line-height:1.5;}
img{border:0;}
html,body{overflow-x:hidden;background-color: #fff;}
ol,ul,li{list-style:none;}
a{color:#333;text-decoration:none;outline:none;}
img{vertical-align:middle;}
*{-webkit-tap-highlight-color:rgba(0,0,0,0);}
.clearfix:after{visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0;}
.lf{
    float: left;
}
.rt{
    float:right;
}
.tab{
    width: 100%;
    height: 2rem;
    font-size: 0.9rem;
    color: #555;
    border-bottom: 0.08rem solid #999;
}
.tab > div{
    width: 50%;
    height: 2rem;
    line-height: 2.1rem;
    text-align: center;
}
.tab_active{
    border-bottom: 0.15rem solid #ed9029;
    color: #000;
}
.supply {
    display: none;
}
.supply .infor{
    height: 7.5rem;
    width: 100%;
    font-size: 0.7rem;
    line-height: 1.5rem;
    color: #333;
    border-bottom: 0.08rem solid #999;
}
.supply .infor .infor_left{
    padding-left: 0.2rem;
}
.supply .infor .time{
    font-size:  0.6rem;
    color: #666;
    font-style: normal;
    float: right;
    margin-right: 1rem;
}
.supply .infor .orange{
    color: #ed9029;
    font-style: normal;
}
.supply .infor a.phone{
    background-color: #ed9029;
    color: #fff;
    float: right;
    margin-right: 1rem;
    height: 1.4rem;
    line-height: 1.4rem;
    width: 3rem;
    text-align: center;
}
.supply .infor a.phone:hover{
    padding:0;
}
.downstream {
}
.downstream .navcontent .hide{
    display: none;
}
.downstream .nav {
    width: 100%;
    float: left;
    height: 1.5rem;
    font-size: 0.7rem;
    line-height: 1.5rem;
}
.downstream .nav li{
    height: 1.5rem;
    font-size: 0.7rem;
    line-height: 1.5rem;
    text-align: center;
    width: 33.3%;
    float: left;
    margin: 0.2rem 0;
    color: #aaa;
    border-bottom: 0.08rem solid #999;
}

.downstream li.nav_active{
    border-bottom: 0.1rem solid #00B2EE;
    color: #000;
}
.downstream .content {
    width: 100%;
    float: left;
}
.content .infore{
    width: 100%;
    margin: 0.2rem 0;
    float: left;
    border-bottom: 0.3rem solid #eee;
}
.infore .title{
    height: 1.2rem;
    color: #666;
    font-size: 0.6rem;
    width: 100%;
    border-bottom: 0.08rem solid #aaa;
}
.infore .title span.rt{
    float: right;
}
.infore .title span.left{
    float: left;
    padding-left: 0.5rem;
    line-height: 1rem;
    /*width: 33%;*/
}
.infore .title span i{
    font-style: normal;
}
.infore .title span.frist{
    margin-right: 15px;
    float: left;
    max-width: 40%;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    padding-top: 0;
}
.infore .title span.last{
    text-align: right;
    padding-right: 0.5rem;
}
.infore .title .name{
    color: #000;
    font-size: 0.7rem;
    font-weight: normal;
}
.infore ul{
    width: 100%;
    float: left;
    font-size: 0.7rem;
    color: #000;
    padding:0.2rem 0;
}
.infore ul li{
    float: left;
    width: 100%;
    height: 1.2rem;
    line-height: 1.2rem;
    padding:0  0.5rem;
}
.infore ul li span{

    float: left;
    width: 15%;
    text-align: left;
}
.infore ul li span:nth-child(1){

    font-weight: bolder;


}



.infore ul li span:nth-child(2){
    width: 70%;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.infore ul li span.orange{
    color: #ed9029;
    text-align: right;
    font-weight: bold;
}
.infore .line{
    width: 100%;
    height: 1.6rem;
    line-height: 1.4rem;
    color: #666;
    font-size: 0.6rem;
    float: left;
    padding: 0.1rem 0.5rem;
    border:0.08rem solid #aaa;
}
.infore .no{
    border-top: 0;
}
.infore .line span{
    width: 33.3%;
    float: left;
}
.infore .line span.last{
    cursor: pointer;
    text-align: right;
    font-weight: bold;
}
.infore .line span.last i{
    font-style: normal;
    color: #000;
}
.replease{
    background:rgba(144,144,144,.7);
    text-align: center;
    font-size: 0.5rem;
    width: 50px;
    height: 50px;
    line-height: 50px;
    border-radius: 25px;
    position: fixed;
    bottom: 100px;
    right: 0;
}
.search{
    display: none;
    background:rgba(144,144,144,.7);
    text-align: center;
    font-size: 0.5rem;
    width: 50px;
    height: 50px;
    line-height: 50px;
    border-radius: 25px;
    position: fixed;
    bottom: 50px;
    right: 0;
}
.restext{
    color: #fff;
}
.searchModel{
    display: none;
    z-index: 5;
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 100%;
    background: rgba(0,0,0,.5);
}
.s-container{
    width: 90%;
    border-radius: 10px;
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background: #fff;
}
.s-top{
    width: 100%;
    height: 2rem;
    text-align: center;
}
.s-content{
    width: 100%;
    padding-left: 1rem;
    padding-right: 1rem;
}
.s-line{
    width: 100%;
    height: 1.5rem;
    line-height: 1.6rem;
}
.s-line:nth-child(3){
    height: 7rem;
}
.s-line .cityselect{
    width: 100%;
    height: 1.5rem;
}
.s-line select{
    width: 60%;
    height: 1.4rem;
}
.s-btn{
    width: 80%;
    height: 2rem;
    margin-left: 10%;
}
.s-btn button{
    color:#fff;
    width: 4rem;
    height: 1.2rem;
    border-radius: 0.1rem;
    border: 0;
    outline: 0;
    background-color: #aaa;
}
.s-btn button:last-child{
    background-color: #f6a85e;
}