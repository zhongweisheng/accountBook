body,button,dd,dt,dl,form,h1,h2,h3,h4,h5,h6,input,li,ol,p,select,table,td,textarea,ul{margin:0;padding:0;}
html{font-size:20px;-ms-touch-action:none;}
body,button,input,select,textarea{font-family:"苹方","圆体-简","方正兰亭细黑简体","微软雅黑",Verdana,Arial,Helvetica,sans-serif;line-height:1.5;}
img{border:0;}
html,body{overflow-x:hidden;background-color: #f6f7f9;}
ol,ul,li{list-style:none;}
a{color:#333;text-decoration:none;outline:none;}
img{vertical-align:middle;}
*{-webkit-tap-highlight-color:rgba(0,0,0,0);}
.clearfix:after{visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0;}
.lf{
    float: left;
}
.rt{
    float:right;
}
.tab{
    width: 100%;
    height: 2rem;
    font-size: 0.9rem;
    background-color: #eceaeb;
}
.tab > div{
    width: 33.3%;
    height: 2rem;
    line-height: 2.1rem;
    text-align: center;
}
.tab_active{
    background-color: #848381;
    color: #fff;
}
.nav{
    width: 100%;
    height: 1.5rem;
    color: #999999;
    margin-top: 0.5rem;
}
.nav{
    background-color: #fff;
}
.nav > ul > li{
    font-size: 0.8rem;
    float: left;
    width: 25%;
    height: 1.5rem;
    line-height: 1.6rem;
    text-align: center;
}
.nav_active{
    border-bottom: 1px solid #1b67a1;
    color: #fcd5ae;
}
.content{
    width: 100%;
    overflow:auto;
}
.card{
    width: 100%;
    margin-top: 0.5rem;
    color: #5c5c5c;
    background-color: #fff;
}
.media{
    width: 20%;
    height: 100%;
}
.media > img{
    width: 90%;
}
.list{
    padding: 0.5rem;
}
.list > ul > li{
    font-size: 0.7rem;
    width:100%;
}
.list > ul > li > span{
    color:#5c5c5c;
}
.date{
    width:30%;
    text-align: center;
    font-size: 0.7rem;
    color: #c1c1c1;
    padding: 0.5rem 0;
}
.date-one{
    width:30%;
    text-align: center;
    font-size: 0.7rem;
    color: #c1c1c1;
    padding: 1rem 0;
}

.spinner {
    height: 2rem;
    text-align: center;
    font-size: 10px;
}

.spinner > div {
    background-color: #ef6272;
    height: 100%;
    width: 6px;
    display: inline-block;

    -webkit-animation: stretchdelay 1.2s infinite ease-in-out;
    animation: stretchdelay 1.2s infinite ease-in-out;
}

.spinner .rect2 {
    -webkit-animation-delay: -1.1s;
    animation-delay: -1.1s;
}

.spinner .rect3 {
    -webkit-animation-delay: -1.0s;
    animation-delay: -1.0s;
}

.spinner .rect4 {
    -webkit-animation-delay: -0.9s;
    animation-delay: -0.9s;
}

.spinner .rect5 {
    -webkit-animation-delay: -0.8s;
    animation-delay: -0.8s;
}

@-webkit-keyframes stretchdelay {
    0%, 40%, 100% { -webkit-transform: scaleY(0.4) }
    20% { -webkit-transform: scaleY(1.0) }
}

@keyframes stretchdelay {
    0%, 40%, 100% {
        transform: scaleY(0.4);
        -webkit-transform: scaleY(0.4);
    }  20% {
           transform: scaleY(1.0);
           -webkit-transform: scaleY(1.0);
       }
}
.more-text{
    height: 1.5rem;
    text-align: center;
    color: #c1c1c1;
    font-size: 0.6rem;
    line-height: 1.5rem;
}
.hiden{
    display: none;
}
.card .more-text{
    cursor: pointer;
}
.top{
    background: #ed9029;
    position: fixed;
    top: 0;
    width: 100%;
    height: 2rem;
    line-height: 2rem;
    text-align: center;
    z-index: 100;
}
.top>h2{
    margin: 0 auto;
    font-weight: normal;
    color: #333333;
    font-size: 1rem;
}
.list ul{
    display: block;
    width: 100%;
}
.list ul li{
    display: block;
    width: 33.3%;
    float: left;
}
.title{
    position: absolute;
    top: 0;
    text-align: center;
    width: 100%;
}
.filter{
    display: none;
    width: 100%;
    height: 6rem;
}
.select {
    cursor: pointer;
    display: none;
    border: 1px solid rgba(204, 204, 204, 1);
    width: 11rem;
    background: white;
    position: absolute;
    top: 2rem;
    left: 50%;
    transform: translateX(-50%);
}
.select li:hover{
    background-color: #eee;
}
.depotChange{
    background-color:#eee;
}
