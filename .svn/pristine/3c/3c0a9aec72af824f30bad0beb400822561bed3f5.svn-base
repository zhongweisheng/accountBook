body,button,dd,dt,dl,form,h1,h2,h3,h4,h5,h6,input,li,ol,p,select,table,td,textarea,ul{margin:0;padding:0;}
html{font-size:20px;-ms-touch-action:none; }
body,button,input,select,textarea{font-family:"苹方","圆体-简","方正兰亭细黑简体","微软雅黑",Verdana,Arial,Helvetica,sans-serif;line-height:1.5;}
img{border:0;}
html,body{overflow-x:hidden;background-color: #eee;}
ol,ul,li{list-style:none;}
a{color:#333;text-decoration:none;outline:none;}
img{vertical-align:middle;}
*{-webkit-tap-highlight-color:rgba(0,0,0,0);}
.clearfix:after{visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0;}
.lf{
    float: left;
}
.rt{
    float:right;
}
.tab{
    width: 100%;
    height: 2rem;
    font-size: 0.8rem;
    color: #7f7f7d;
    background-color:white;
    position: fixed;
    bottom: 0;
}
.tab div{

    width: 33.3%;
    height: 49px;
    margin-top: 5px;
    text-align: center;
    border-bottom: 0.05rem solid #dddddf;
    font-size: 10px;
}
.tab_active{


}
.top{
    width: 100%;
    height: 2.4rem;
    position: fixed;
    top: 0;
    background-color: #eee;
}
.nav_ul{
    width: 100%;
    float: left;
    height: 1.4rem;
    font-size: 0.7rem;
    line-height: 1.4rem;
    margin-top: 0.5rem;
    border-top: 0.05rem solid #dddddf;
    background-color: #fff;
}
.nav_ul .nav li{
    height: 1.4rem;
    font-size: 0.7rem;
    line-height: 1.4rem;
    text-align: center;
    width: 33.3%;
    float: left;
    color: #686868;
    border-bottom: 0.05rem solid #ddd;
}
.nav_ul li.nav_active{
    border-bottom: 0.08rem solid #fd9a18;
    color: #fd9a18;
}
.top_search{
    margin-top: 0.5rem;
    height: 1.4rem;
    line-height: 1.4rem;
    width: 100%;
    background-color: #ff9b17;
    float: left;
    padding:0 0.5rem;
}
.top_search li{
    width: 50%;
    height: 1.4rem;
    line-height: 1.4rem;
    float: left;
    color: #fff;
    font-size: 0.7rem;
}
.top_search li:nth-child(2){
    width: 50%;
    text-align: center;
    background-size: 1.2rem 1.2rem;
    background-image: url(../img/downstream.png);
    background-repeat: no-repeat;
    background-position: right center;
    text-align: left;
}
.top_search li:nth-child(3){
    width: 30%;
    font-size:0.6rem;
    text-align: right;
    background-size: 0.7rem 0.7rem;
    background-image: url(../img/search.png);
    background-repeat: no-repeat;
    background-position: right center;
    padding-right: 0.8rem;
}
.navcontent{
    padding-top: 2.5rem;
    margin-bottom: 4rem;
    width: 100%;
    font-size: 0.7rem;
    color: #686868;
    float: left;
}
.content .infor{
    background-color: #fff;
    width: 100%;
    float: left;
    margin-bottom: 0.5rem;
}
.infor .title{
    height: 1.7rem;
    width: 100%;
    line-height: 1.3rem;
    color: #726e6f;
    border-bottom: 0.05rem solid #ddd;
    padding:0.4rem 0.5rem 0 0.5rem;
    float: left;
    font-size: 0.6rem;
}
.infor .title .name{
    font-style: normal;
    color: #000;
    font-size: 0.7rem;
}
.infor ul{
    font-size:0.7rem;
    line-height: 2.2rem;
    height: 2.2rem;
    float: left;
    width: 100%;
    border-bottom: 0.05rem solid #ddd;
}
.infor ul span{
    text-align: left;
    float: left;
    height: 2rem;
    line-height: 2rem;
    font-size: 0.65rem;
}
.infor ul li span:nth-child(1){
    padding-left: 0.5rem;
    width: 20%;
    float: left;
    color: #000;
}
.infor ul li span:nth-child(2){
    width: 60%;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    float: left;
}
.infor ul li span.orange{
    color: #fd9a18;
    font-weight: bold;
    width: 20%;
    text-align: right;
    padding-right:0.5rem;
}
.infor .distance{
    width: 100%;
    float: left;
    height: 1.4rem;
    line-height: 1.4rem;
    font-size: 0.6rem;
    padding-right: 0.5rem;
}
.infor .distance .time{
    float: right;
    width: auto;

}
.infor .distance .long{
    float: right;
    padding:0 0.7rem;
    background-size: 0.5rem 0.7rem;
    background-image: url(../img/address.png);
    background-repeat: no-repeat;
    background-position: left center;
    width: 40%;
}
.replease{
    background:rgba(144,144,144,.7);
    text-align: center;
    font-size: 0.5rem;
    width: 50px;
    height: 50px;
    line-height: 50px;
    border-radius: 25px;
    position: fixed;
    bottom: 155px;
    right: 0;
}
.search{
    background:rgba(144,144,144,.7);
    text-align: center;
    font-size: 0.5rem;
    width: 50px;
    height: 50px;
    line-height: 50px;
    border-radius: 25px;
    position: fixed;
    bottom: 105px;
    right: 0;
}
.pushMsg{
    background:rgba(144,144,144,.7);
    text-align: center;
    font-size: 0.5rem;
    width: 50px;
    height: 50px;
    line-height: 50px;
    border-radius: 25px;
    position: fixed;
    bottom: 205px;
    right: 0;
}
.restext{color:#fff;font-size: 12px;}
.searchModel{
    display: none;
    z-index: 5;
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 100%;
    background: rgba(0,0,0,.5);
}
.s-container{
    font-size: 16px;

    width: 90%;
    border-radius: 10px;
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background: #fff;
}
.s-top{
    line-height: 2rem;
    width: 100%;
    height: 2rem;
    text-align: center;
}
.s-content{
    width: 100%;
    padding-left: 1rem;
    padding-right: 1rem;
}
.s-line{
    color: #6c6c6c;
    width: 100%;
    height: 1.5rem;
    line-height: 1.6rem;
}
.s-line:nth-child(2){
    height: 4.5rem;
    margin-bottom: 16px;
}
.s-line .cityselect{
    width: 100%;
    height: 1.5rem;
}
.s-line select{
    width: 60%;
    height: 1.4rem;
}
.s-btn{
    width: 80%;
    height: 2rem;
    margin-left: 10%;
}
.s-btn button{
    color: #fff;
    width: 5rem;
    height: 1.5rem;
    border-radius: 0.1rem;
    border: 0;
    outline: 0;
    background-color: #169bd5;
}
.s-btn button:last-child{
    background-color: #f7941e;
}

.card-head{
    border-bottom: 1px solid #ddd;
}
.card-head b{
    width: 1.3rem;
    height: 1.3rem;
    display: inline-block;
    margin-left: -30%;
    background: url("../img/applyed.png") no-repeat;
    background-size: 100%;
    z-index: 10;
    cursor: pointer;
}
.card-head,.card-footer{
    width: 96%;
    margin: 0 auto;
    height: 1.5rem;
    line-height: 1.5rem;
}

.card-content i{color: #f6a85e;}
.item-inner{
    height: 1.5rem;
    width: 50%;
}
.gray{
    font-size: 12px;
    color: #aaa;
}

.white-space{
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.content-inner{
    width: 100%;
    height: 1.5rem;
    line-height: 1.5rem;
}
.selficon{
    vertical-align: middle;
    display: inline-block;
    width: 32px;
    height: 32px;
}
.icon-purchase-active{
    background: url('../img/求购选中.png') no-repeat center;
    background-size: 100%;
}
.icon-apply{
    background: url('../img/供应商.png') no-repeat center;
    background-size: 100%;
}
.icon-my{
    background: url('../img/服务.png') no-repeat center;
    background-size: 100%;
}


/******弹推送********/
.balanceModel,.balanceModel1{
    width: 100%;
    height: 100%;
	background: rgba(0,0,0,.1);
	font-size: 14px;
}
.b-content{
	height: 450px;
    width: 300px;
    border-radius: 10px;
    background-color: #f4f5f9;
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}
.b-content ul li {
	text-align: center;
	margin: 8px auto;
}
.b-content ul li span{
	margin: 0 10px;
	background: orange;
	color: white;
}
.type-content ul li {
	float: left;
	margin-right: 1px;
}
.isshowp{
    float: left;
    margin-top: 20px;
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    position: relative;
}
.addressSelete{
    float: right;
    width: 74%;
}


