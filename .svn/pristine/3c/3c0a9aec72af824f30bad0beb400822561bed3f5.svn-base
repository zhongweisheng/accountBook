.userPic {
    width: 60px;
    height: 60px;
    border: 1px solid #8e8e93;
    border-radius: 50%;
}
.userPic img{
    width: 60px;
    height: 60px;
    border-radius: 50%;
}
.userName {
    width: 100%;
    height: 1rem;
    text-align: center;
}
