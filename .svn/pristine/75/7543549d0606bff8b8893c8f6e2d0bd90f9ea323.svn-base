body,button,dd,dt,dl,form,h1,h2,h3,h4,h5,h6,input,li,ol,p,select,table,td,textarea,ul{margin:0;padding:0;}
html{font-size:20px;-ms-touch-action:none;}
body,button,input,select,textarea{font-family:"苹方","圆体-简","方正兰亭细黑简体","微软雅黑",Verdana,Arial,Helvetica,sans-serif;line-height:1.5;}
img{border:0;}
html,body{overflow-x:hidden;background-color: #f9f3f3;}
ol,ul,li{list-style:none;}
a{color:#333;text-decoration:none;outline:none;}
img{vertical-align:middle;}
*{-webkit-tap-highlight-color:rgba(0,0,0,0);}
.clearfix:after{visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0;}
.lf{
    float: left;
}
.rt{
    float:right;
}
.content{
    width: 98%;
    overflow: auto;
    padding: 0 1%;
}
.tab-container {
    width: 100%;
    height: 40px;
    margin: 0 auto;
    position: fixed;
    top: 0;
    background-color: #fff;
    z-index: 1000;
}
.tab-col{
    float: left;
    width: 33.3%;
    text-align: center;
}
.selectReset{
    width: 80%;
    height: 30px;
    margin: 0;
    padding: 0;
    outline: 0;
}
.container{
    width: 100%;
}
.c-container{
    width: 100%;
    height: 720px;
}
.tableHead{
    width: 100%;
    text-align: center;
    height: 2rem;
    line-height: 2rem;
    font-size: 0.9rem;
    color:#fff;
    background-color: #f6a85e;
}
/*thead{
    background: #aaa;
}*/
#c-pie,#c-inLine,#c-inBar,#c-hasBar,#c-outLine,#c-outBar{
    margin: 0 auto;
}
canvas{
    margin: 10px 0;
}
.height2rem{
    height: 2rem;
    line-height: 2rem;
}
.height1rem{
    height: 1rem;
    line-height: 1rem;
}
.card{
    background: #fff;
    border-bottom: 1px solid #f9f3f3;
    color: #7e7e7e;
    font-size: 0.6rem;
    padding-left: 0.5rem;
}
.cardSpan{
    display: inline-block;
    width: 50%;
    float: left;
}
.innerSpan{
    display: inline-block;
    width: 48%;
}
