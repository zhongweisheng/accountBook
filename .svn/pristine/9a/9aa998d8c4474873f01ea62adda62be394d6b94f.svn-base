.right {
    float: right;
    width: calc(100% - 160px);
    width: -moz-calc(100% - 160px);
    width: -webkit-calc(100% - 160px);
    height: 760px;
    border-left: 1px solid #000;
}
.rightHead{
    margin-top: 100px;
}
.rightContent{
    width: 1000px;
    margin-left: 39px;
    margin-top: 55px;
}
.head-container{
    width: 800px;
    height: 2rem;
    margin: 0 auto;
    border: 0 solid #000;
    border-radius: 5px;
    background-color: #828282;
}
.head-tab{
    width: 33.3%;
    height: 2rem;
    line-height: 2rem;
    text-align: center;
    color: #fff;
    border-radius: 5px;
    cursor: pointer;
}
.head-tab:hover{
    background-color: #c58b4b;
}
.head-tab-active{
    background-color: #c58b4b;
}

table {
    font-size: 16px;
    width: 100%;
    border: 0;
    margin: 0;
    padding: 0;
    color: #000;
    border-collapse: collapse;
}

thead {
    width: 100%;
    height: 40px;
    background-color: #717171;
}
thead > tr > td {
    color: #fff;
    height: 40px;
    background-color: #b4b4b4;
}
tbody > tr:nth-child(odd) {

    background-color: #faf5f9;
}

tbody > tr:nth-child(even) {
    background-color: white;
}

tbody > tr > td {
    height: 40px;
    line-height: 40px;
}
tbody > tr > td:last-child > button{
    cursor: pointer;
    width: 50px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    background-color: #f6a85e;
    border: 0;
    outline: 0;
    color: #fff;
    margin-left: 7px;
}
.leftborder{
    border-left: solid 1px black;
}