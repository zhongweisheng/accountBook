.right {
    float: right;
    width: calc(100% - 160px);
    width: -moz-calc(100% - 160px);
    width: -webkit-calc(100% - 160px);
}

.rightHead {
    margin-top: 20px;
}

.rightHead > ul > li > span {
    color: red;
}

.rightHead > ul > li:nth-child(1) {
    text-align: center;
    font-size: 24px;
    color: #000;
}


.excelBtn {
    cursor: pointer;
    display: inline-block;
    width: 120px;
    height: 34px;
    line-height: 34px;
    margin-left: 50px;
    text-align: center;
    background-color: #f6a85e;
    color: #fff;
    font-size: 20px;
    border: none;
    outline: none;
}
#grainType {
    width: 100px;
    height: 26px;
}

.rightHead > ul > li:nth-child(3) {
    width: 100%;
}

.rightHead > ul > li > div {
    width: 80%;
    margin: 10px auto;
}

.rightHead > ul > li:nth-child(3) span {
    text-align: center;
    float: left;
    width: 20%;
    height: 34px;
    line-height: 34px;
    color: #fff;
}
.rightHead > ul > li span > a{
    cursor: pointer;
    display: inline-block;
    width: 100px;
    height: 34px;
    line-height: 34px;
}

.watchActive{
    background-color: #44aac3;
    color: white;

}
.rightContent {
    width: 100%;
    height: 470px;
    margin-top: 30px;
    margin-left: 39px;
}

table {
    font-size: 16px;
    width: 100%;
    border: 0;
    margin: 0;
    padding: 0;
    color: #000;
    border-collapse: collapse;
}

thead {
    width: 100%;
    height: 40px;
    background-color: #717171;
}
thead > tr > td {
    color: #fff;
    height: 40px;
    background-color: #b4b4b4;
}
tbody > tr:nth-child(odd) {

    background-color: #faf5f9;
}

tbody > tr:nth-child(even) {
    background-color: white;
}

tbody > tr > td {
    height: 40px;
    line-height: 40px;
}
tbody > tr > td:last-child > button{
    cursor: pointer;
    width: 50px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    background-color: #f6a85e;
    border: 0;
    outline: 0;
    color: #fff;
    margin-left: 7px;
}
.leftborder{
    border-left: solid 1px black;
}