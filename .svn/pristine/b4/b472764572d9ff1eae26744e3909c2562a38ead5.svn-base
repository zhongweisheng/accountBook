body,button,dd,dt,dl,form,h1,h2,h3,h4,h5,h6,input,li,ol,p,select,table,td,textarea,ul{margin:0;padding:0;}
html{font-size:24px;-ms-touch-action:none; }
body,button,input,select,textarea{font-family:"苹方","圆体-简","方正兰亭细黑简体","微软雅黑",Verdana,Arial,Helvetica,sans-serif;line-height:1.5;}
img{border:0;}
html,body{overflow-x:hidden;background-color: #fff;}
ol,ul,li{list-style:none;}
a{color:#333;text-decoration:none;outline:none;}
img{vertical-align:middle;}
*{-webkit-tap-highlight-color:rgba(0,0,0,0);}
.clearfix:after{visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0;}
i{font-style: normal;}
.lf{  float: left;  }
.rt{  float:right;  }
.tab{
    width: 100%;
    height: 40px;
    font-size: 0.8rem;
    color: #7f7f7d;
    background-color: #eee;
    position: fixed;
    bottom: 0;
}
.tab div{
    width: 33.3%;
    height: 49px;
    margin-top: 5px;
    text-align: center;
    border-bottom: 0.05rem solid #dddddf;
    font-size: 10px;
}

.replease{
    background:rgba(144,144,144,.7);
    text-align: center;
    font-size: 0.5rem;
    width: 50px;
    height: 50px;
    line-height: 50px;
    border-radius: 25px;
    position: fixed;
    bottom: 155px;
    right: 0;
}
.search{
    background:rgba(144,144,144,.7);
    text-align: center;
    font-size: 0.5rem;
    width: 50px;
    height: 50px;
    line-height: 50px;
    border-radius: 25px;
    position: fixed;
    bottom: 105px;
    right: 0;
}
.pushMsg{
    background:rgba(144,144,144,.7);
    text-align: center;
    font-size: 0.5rem;
    width: 50px;
    height: 50px;
    line-height: 50px;
    border-radius: 25px;
    position: fixed;
    bottom: 205px;
    right: 0;
}
.restext{color:#fff;font-size: 12px;}
.searchModel{
    font-size: 0.9rem;
    display: none;
    z-index: 100;
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 100%;
    background: rgba(0,0,0,.5);
}
.s-container{
    font-size: 16px;
    width: 90%;
    border-radius: 10px;
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background: #fff;
}
.s-top{
    line-height: 2rem;
    width: 100%;
    height: 2rem;
    text-align: center;
}
.s-content{
    width: 100%;
    padding-left: 1rem;
    padding-right: 1rem;
}
.s-line{
    color: #6c6c6c;
    width: 100%;
    height: 1.5rem;
    line-height: 1.6rem;
}
.s-line:nth-child(3){
    height: 4.5rem;
}
.s-line .cityselect{
    width: 96%;
    height: 1.5rem;
}
.s-line select{
    width: 60%;
    height: 1.4rem;
}
.s-btn{
    width: 80%;
    height: 2rem;
    margin-left: 10%;
    margin-top: 16px;
}
.s-btn button{
    color:#fff;
    width: 4rem;
    height: 1.2rem;
    border-radius: 0.1rem;
    border: 0;
    outline: 0;
    background-color: #169bd5;
}
.s-btn button:last-child{
    background-color: #f7941e;
}
.card1{
    width: 100%;
    margin-bottom: 0.5rem;
    background-color: #fff;
}
.card-head{
    border-bottom: 1px solid #ddd;
}
.card-head b{
    width: 1.3rem;
    height: 1.3rem;
    display: inline-block;
    margin-left: -30%;
    background: url("../img/applyed.png") no-repeat;
    background-size: 100%;
    z-index: 10;
    cursor: pointer;
}
.card-head,.card-footer{
    width: 96%;
    margin: 0 auto;
    height: 1.5rem;
    line-height: 1.5rem;
}

.card-content i{color: #f6a85e;}
.item-inner{

    height: 1.5rem;
    width: 50%;
}
.gray{
    font-size: 12px;
    color: #aaa;
}

.white-space{
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.content-inner{
    width: 100%;
    height: 1.5rem;
    line-height: 1.5rem;
}
.selficon{
    margin: 0 auto;
    vertical-align: middle;
    display: block;
    width: 21px;
    height: 21px;
}
.icon-purchase{
    background: url('../img/求购.png') no-repeat center;
    background-size: 100%;
}
.icon-apply-active{
    background: url('../img/供应商选中.png') no-repeat center;
    background-size: 100%;
}
.icon-my{
    background: url('../img/服务.png') no-repeat center;
    background-size: 100%;
}
/******弹推送********/
.balanceModel,.balanceModel1{
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.1);
    font-size: 14px;
}
.b-content{
	height: 460px;
    width: 300px;
    border-radius: 10px;
    background-color: #f4f5f9;
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}
.b-content ul li {
	text-align: center;
	margin: 8px auto;
}
.b-content ul li span{
	margin: 0 10px;
	background: orange;
	color: white;
}
.type-content ul li {
	float: left;
	margin-right: 1px;
}
.isshowp{
    float: left;
    margin-top: 20px;
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    position: relative;
}
#searchName{
    width: 61%;
    height: 1.4rem;

}
.addressSelete{
        float: right;
    width: 74%;
}
