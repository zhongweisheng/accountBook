.right {
    float: right;
    width: calc(100% - 160px);
    width: -moz-calc(100% - 160px);
    width: -webkit-calc(100% - 160px);
}
.tab-container {
    width: 100%;
    height: 40px;
    margin: 40px auto;
}
.tab-col{
    float: left;
    width: 20%;
    text-align: center;
}
.tab-btn {
    cursor: pointer;
    width: 80px;
    height: 30px;
    text-align: center;
    margin: 0 auto;
}

.tab-btn-active {
    background: #44aac3;
    cursor: pointer;
    width: 80px;
    height: 30px;
    color: #fff;
    text-align: center;
    margin: 0 auto;
}
.selectReset{
    width: 80px;
    height: 30px;
    margin: 0;
    padding: 0;
    outline: 0;
}
.rightContent{
    width: 1000px;
    margin-left: 39px;
}
.c-container{
    width: 500px;
    height: 800px;
    float: left;
}
.ctop{
    width: 100%;
    height: 2rem;
    line-height: 2rem;
    font-size: 0.8rem;
    font-weight: bold;
    text-align: center;
}
.cmiddle{
    height: 140px;
}
.cfooter{
    height: 190px;
}
.cfleft{
    margin-top: 0.4rem;
    font-size: 0.7rem;
    text-indent: 0.2rem;
    width: 50px;
}
.cfright{
    width:448px;
}
.fontbig{
    font-size: 0.8rem;
    text-indent: 0.2rem;
    height: 60px;
    line-height: 60px;
}
.fontSmall{
    font-size: 0.7rem;
    text-indent: 0.2rem;
    height: 40px;
    line-height: 40px;
}
.cmline{
    color: #777;
    /*display: inline-block;
    width: 60px;
    line-height: normal;
    text-align: center;*/
}
table{
    width: 100%;
    font-size: 16px;
    text-align: left;
}
thead{
    background: #aaa;
}
