//得到粮库
var test;
var depotre = Restful.post('bsinventorydepot/getDepots', {userId: userId});
test = depotre[0].id;
//切换粮库
$('.depot-slider').on('click', '.depot', function () {
    $(this).removeClass('depot-active');
