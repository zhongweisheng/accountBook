.right {
    float: left;
    width: calc(100% - 160px);
    width: -moz-calc(100% - 160px);
    width: -webkit-calc(100% - 160px);
}

.rightHead {
    margin-top: 20px;
}

.rightHead > ul > li > span {
    color: red;
}

.rightHead > ul > li:nth-child(1) {
    text-align: center;
    font-size: 24px;
    color: #000;
}

.rightHead > ul > li:nth-child(2) {
    text-align: right;
    margin-bottom: 20px;
}

.excelBtn {
    cursor: pointer;
    display: inline-block;
    width: 120px;
    height: 34px;
    line-height: 34px;
    margin-left: 50px;
    text-align: center;
    background-color: #f6a85e;
    color: #fff;
    font-size: 20px;
    border: none;
    outline: none;
}
#grainType {
    width: 100px;
    height: 26px;
}

.rightHead > ul > li:nth-child(3) {
    width: 100%;
}

.rightHead > ul > li > div {
    width: 80%;
    margin: 10px auto;
}

.rightHead > ul > li span {
    text-align: center;
    float: left;
    width: 25%;
    height: 34px;
    line-height: 34px;
    color: #fff;
}
.rightHead > ul > li span > a{
    cursor: pointer;
    display: inline-block;
    width: 100px;
    height: 34px;
    line-height: 34px;
    color: #fff;
}
.watch{
    background-color: #f6a85e;
}
.watchActive{
    background-color: #44aac3;
}
.rightContent {
    width: 1000px;
    height: 450px;
    margin-top: 30px;
    margin-left: 39px;
}

table {
    font-size: 16px;
    width: 100%;
    border: 0;
    margin: 0;
    padding: 0;
    color: #000;
    border-collapse: collapse;
}

thead {
    width: 100%;
    height: 40px;
    background-color: #717171;
}

thead > tr > td {
    color: #fff;
    height: 40px;
    background-color: #b4b4b4;
    text-align: center;
}

tbody > tr:nth-child(odd) {
    background-color: #faf5f9;
}

tbody > tr:nth-child(even) {
    background-color: #white;
}

tbody > tr > td {
    text-align: center;
    height: 40px;
    line-height: 40px;
}
tbody > tr > td:last-child > button{
    cursor: pointer;
    width: 50px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    background-color: #f6a85e;
    border: 0;
    outline: 0;
    color: #fff;
}
.registerModel{
    top:0;
    left: 0;
    position: fixed;
    width: 100%;
    height: 100%;
    z-index: 100;
    display: none;
    background: rgba(189,189,189,.7);
}
.findPassWordModel{
    top:0;
    left: 0;
    position: fixed;
    width: 100%;
    height: 100%;
    z-index: 100;
    display: none;
    background: rgba(189,189,189,.7);
}
.register-form{
    width: 680px;
    height:610px;
    background-color: white;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -305px;
    margin-left: -340px;
}
.register-form .content{
    width: 70%;
    margin: auto;
}
.register-form-title {
    padding-top: 50px;
}
.register-form-title .list1{
    border:solid 1px #44aac3;
    width: 100%;
}
.register-form-title .list2 span{
    color: #44aac3;
    font-size: 30px;
}
.field-name{
    float: left;
    display: block;
    width: 20%;
    font-size: 20px;
    color: #858384;
    line-height: 40px;
}
.form input{
    padding-left: 2%;
    float: left;
    display: block;
    width: 75%;
    height: 35px;
}
.register-btn{
    width: 100%;
    height: 38px;
    margin: 0 auto;
    margin-top: 20px;
}
.register-btn a{
    background-color: #44aac3;
    text-align: center;
    display: block;
    width: 40%;
    float: left;
    color: white;
    font-size: 25px;
    cursor: pointer;
}
.phonecode {
    border: solid 1px #858384;
    float: left;
    display: block;
    width: 79%;
    height: 35px;
}
.phonecode input{
    width: 50%;
    padding-left: 2%;
    border-style: none;
}
.getPhoneCode{
    display: block;
    color: #44aac3;
    font-size: 20px;
    line-height: 35px;
    text-align: right;
    padding-right: 10px;
    cursor: pointer;
}
.disable{
    opacity: .5;
}
.reminder{
    display: none;
    color: red;
    height: 12px;
    font-size: 12px;
    text-align: right;
}
.list1{
    width: 100%;
    border-bottom: solid 1px #d2d2d2;
    margin-bottom: -30px;
}
.list2{
    width: 100%;
}
.list2 {
    display: block;
    font-size: 40px;
    width: 100%;
    text-align: center;
}
.list2 span{
    color: #6d6d6d;
    background: white;
    padding:0 50px;
}
.form textarea{
    width: 77%;
    height: 70px;
}