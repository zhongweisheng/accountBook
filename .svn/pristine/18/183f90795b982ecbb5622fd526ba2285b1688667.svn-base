.right {
    float: right;
    width: calc(100% - 160px);
    width: -moz-calc(100% - 160px);
    width: -webkit-calc(100% - 160px);
}

.rightHead {
    margin-top: 20px;
}

.rightHead > ul > li > span {
    color: red;
}

.rightHead > ul > li:nth-child(1) {
    text-align: center;
    font-size: 24px;
    color: #f5a85d;
}

.rightHead > ul > li:nth-child(2) {
    text-align: right;
    margin-bottom: 20px;
}

.excelBtn {
    display: inline-block;
    width: 120px;
    height: 34px;
    line-height: 34px;
    margin-left: 50px;
    text-align: center;
    background-color: #f6a85e;
    color: #fff;
    font-size: 20px;
}

#grainType {
    width: 100px;
    height: 26px;
}

.rightHead > ul > li:nth-child(3) {
    width: 100%;
}

.rightHead > ul > li > div {
    width: 80%;
    margin: 10px auto;
}

.rightHead > ul > li span {
    text-align: center;
    float: left;
    width: 25%;
    height: 34px;
    line-height: 34px;
    color: #fff;
}
.rightHead > ul > li span > a{
    cursor: pointer;
    display: inline-block;
    width: 100px;
    height: 34px;
    line-height: 34px;
    color: #fff;
}
.watch{
    background-color: #f6a85e;
}
.watchActive{
    background-color: #44aac3;
}
.rightContent {
    width: 1000px;
    height: 450px;
    margin-top: 30px;
    margin-left: 39px;
}

table {
    font-size: 16px;
    width: 100%;
    border: 0;
    margin: 0;
    padding: 0;
    color: #000;
    border-collapse: collapse;
}

thead {
    width: 100%;
    height: 40px;
    background-color: #44aac3;
}

thead > tr > td {
    color: #fff;
    height: 40px;
    background-color: #44aac3;
    text-align: center;
}

tbody > tr:nth-child(odd) {
    background-color: #faf5f9;
}

tbody > tr:nth-child(even) {
    background-color: #8dd1e4;
}

tbody > tr > td {
    text-align: center;
    height: 40px;
    line-height: 40px;
}
.leftborder{
    border-left: solid 1px black;
}