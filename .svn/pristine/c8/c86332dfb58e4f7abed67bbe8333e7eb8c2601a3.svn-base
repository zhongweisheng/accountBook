.right {
    float: right;
    width: calc(100% - 160px);
    width: -moz-calc(100% - 160px);
    width: -webkit-calc(100% - 160px);
}

.rightHead {
    margin-top: 20px;
}

.rightHead > ul > li > span {
    color: red;
}

.rightHead > ul > li:nth-child(1) {
    text-align: center;
    font-size: 24px;
    color: #000;
}


.excelBtn {
    cursor: pointer;
    display: inline-block;
    width: 120px;
    height: 34px;
    line-height: 34px;
    margin-left: 50px;
    text-align: center;
    background-color: #f6a85e;
    color: #fff;
    font-size: 20px;
    border: none;
    outline: none;
}
#grainType ,#depot {
    width: 100px;
    height: 28px;
}

.rightHead > ul > li:nth-child(3) {
    width: 100%;
}

.rightHead > ul > li > div {
    width: 80%;
    margin: 10px auto;
}

.rightHead > ul > li:nth-child(3) span {
    text-align: center;
    float: left;
    width: 20%;
    height: 34px;
    line-height: 34px;
    color: #fff;
}
.rightHead > ul > li span > a{
    cursor: pointer;
    display: inline-block;
    width: 100px;
    height: 34px;
    line-height: 34px;
}

.watchActive{
    background-color: #44aac3;
    color: white;

}
.rightContent {
    width: 100%;
    height: 470px;
    margin-top: 30px;
    margin-left: 39px;
}

table {
    font-size: 16px;
    width: 100%;
    border: 0;
    margin: 0;
    padding: 0;
    color: #000;
    border-collapse: collapse;
}

thead {
    width: 100%;
    height: 40px;
    background-color: #717171;
}
thead > tr > td {
    color: #fff;
    height: 40px;
    background-color: #b4b4b4;
}
tbody > tr:nth-child(odd) {

    background-color: #faf5f9;
}

tbody > tr:nth-child(even) {
    background-color: white;
}

tbody > tr > td {
    height: 40px;
    line-height: 40px;
}
tbody > tr > td:last-child > button{
    cursor: pointer;
    width: 50px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    background-color: #f6a85e;
    border: 0;
    outline: 0;
    color: #fff;
    margin-left: 7px;
}

.balanceModel{
    width: 100%;
    height: 100%;
    position: fixed;
    top:0;
    left:100%;
    background: rgba(0,0,0,.5);
}
.b-content{
    width: 500px;
    background-color: #f4f5f9;
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}
.b-yes{
    left: 150%;
}
.card{
    width: 100%;
    background-color: #fff;
    margin-bottom: 10px;
}
.b-item{
    height: 40px;
    line-height: 40px;
    width: 100%;
    border-top:1px solid #eee;
}
.inner-l{
    padding-left: 20px;
    display: inline-block;
    width: 50%;
}
.inner-r{
    display: inline-block;
    width: 48%;
}
.inner50{
    display: inline-block;
    text-align: center;
    width: 48%;
}
#b-cencel,#b-back{
    border: 0;
    outline: 0;
    width: 150px;
    height: 30px;
    background: #aaa;
    color: #fff;
}
#b-confirm,#b-submit{
    border: 0;
    outline: 0;
    width: 150px;
    height: 30px;
    background: #f6a85e;
    color: #fff;
}
