<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <style type="text/css">
        * {
            margin: 0;
            padding: 0;
        }

        .leftSider {
            width: 200px;
            height: 800px;
            border: 1px solid #000;
        }

        .parent {
            width: 100%;
            height: 40px;
        }

        .child p {
            width: 100%;
            height: 30px;
            background-color: #4cae4c;
            border-bottom: 1px solid #000;
        }
    </style>
</head>
<body>
<div class="leftSider">
    <div class="leftContent">

    </div>
</div>
<script src="http://www.jq22.com/jquery/jquery-2.1.1.js"></script>
<script type="text/javascript">
    $.ajax({
        type: 'get',
        url: 'accountData/json/leftSide.json',
        dataType: 'json',
        timeout: 5000,
        async: false,
        success: function (res) {
            var html = '';
            for (var i = 0, len = res.data.length; i < len; i++) {
                html += '<div class="parent">' + res.data[i].parent + '</div><div class="child" style="display: block">';
                var datachild = res.data[i].children;
                for (var j = 0; j < datachild.length; j++) {
                    html += '<p>' + datachild[j] + '</p>';
                }
                html += '</div>';
                $(".leftContent").empty();
                $(".leftContent").append(html);
            }
        }
    });
    $(document).on('click','.parent',function () {
        $(this).next(".child").toggle();
    });
</script>
</body>
</html>